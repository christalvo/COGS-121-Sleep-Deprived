{
"name": "Getting Started Example",
"version": "1.0",
"description": "Build an Extension",
"permissions": ["activeTab", "declarativeContent", "storage"],
"background": {
  "scripts": ["background.js", "jquery-3.3.1.min.js"],
  "persistent": false
},
"content_scripts": [
  {
    "js": ["jquery-3.3.1.min.js"],
    "css": ["style.css"],
    "matches": ["<all_urls>"]
  }
],
"page_action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "public/images/get_started16.png",
    "32": "public/images/get_started32.png",
    "48": "public/images/get_started48.png",
    "128": "public/images/get_started128.png"
  }
},
"icons": {
  "16": "public/images/get_started16.png",
  "32": "public/images/get_started32.png",
  "48": "public/images/get_started48.png",
  "128": "public/images/get_started128.png"
},
"options_page": "public/settings.html",
"manifest_version": 2
}
